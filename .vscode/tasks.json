{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"build"
			],
			"problemMatcher": [
				"$tsc"
			],
			"group": "build"
		},
		{
			"label": "run-cli",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"run",
				"--project",
				"src/CsharpDialog.CLI"
			],
			"group": "build"
		}
	]
}